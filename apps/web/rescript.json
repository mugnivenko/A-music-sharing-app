{
  "name": "web",
  "sources": {
    "dir": "src",
    "subdirs": true
  },
  "package-specs": {
    "module": "es6-global",
    "in-source": true
  },
  "suffix": ".res.js",
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react",
    "@repo/rescript-mui-system",
    "@repo/rescript-mui-material",
    "@repo/rescript-next-intl"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "jsx": {
    "mode": "automatic",
    "version": 4
  }
}